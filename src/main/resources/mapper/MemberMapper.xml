<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="toyProject.toyProject01.bord.mapper.MemberMapper">
    <insert id="insertMember" parameterType="toyProject.toyProject01.bord.mapper.MemberMapper">
        INSERT INTO member (id, pw, nickname, age, tel, email)
        VALUES (#{id}, #{pw}, #{nickname}, #{age}, #{tel}, #{email})
    </insert>
</mapper>
